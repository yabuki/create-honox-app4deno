{
  "tasks": {
    "dev": "deno run --watch main.ts",
    "test": "deno test --allow-all",
    "lint": "deno lint",
    "fmt": "deno fmt"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1"
  },
  "lint": {
    "include": ["src/", "tests/"],
    // "exclude": ["src/testdata/", "src/fixtures/**/*.ts"],
    "rules": {
      "tags": ["recommended"],
      "include": ["eqeqeq"]
      // "exclude": ["no-unused-vars"]
    }
  }
}
